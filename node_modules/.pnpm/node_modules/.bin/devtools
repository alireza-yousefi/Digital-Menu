#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/project/Digital-Menu/node_modules/.pnpm/@nuxt+devtools@1.6.0_rollup@4.24.2_vite@5.4.10_@types+node@22.8.1_terser@5.36.0__vue@3.5.12/node_modules/@nuxt/devtools/node_modules:/mnt/d/project/Digital-Menu/node_modules/.pnpm/@nuxt+devtools@1.6.0_rollup@4.24.2_vite@5.4.10_@types+node@22.8.1_terser@5.36.0__vue@3.5.12/node_modules/@nuxt/node_modules:/mnt/d/project/Digital-Menu/node_modules/.pnpm/@nuxt+devtools@1.6.0_rollup@4.24.2_vite@5.4.10_@types+node@22.8.1_terser@5.36.0__vue@3.5.12/node_modules:/mnt/d/project/Digital-Menu/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/project/Digital-Menu/node_modules/.pnpm/@nuxt+devtools@1.6.0_rollup@4.24.2_vite@5.4.10_@types+node@22.8.1_terser@5.36.0__vue@3.5.12/node_modules/@nuxt/devtools/node_modules:/mnt/d/project/Digital-Menu/node_modules/.pnpm/@nuxt+devtools@1.6.0_rollup@4.24.2_vite@5.4.10_@types+node@22.8.1_terser@5.36.0__vue@3.5.12/node_modules/@nuxt/node_modules:/mnt/d/project/Digital-Menu/node_modules/.pnpm/@nuxt+devtools@1.6.0_rollup@4.24.2_vite@5.4.10_@types+node@22.8.1_terser@5.36.0__vue@3.5.12/node_modules:/mnt/d/project/Digital-Menu/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@nuxt/devtools/cli.mjs" "$@"
else
  exec node  "$basedir/../@nuxt/devtools/cli.mjs" "$@"
fi
